{
  "name": "unserious-cm",
  "version": "0.0.1",
  "description": "A delightfully whimsical cookie manager that brings a touch of fun to your browsing experience!",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "build:chrome": "vite build",
    "build:firefox": "BROWSER_TARGET=firefox vite build",
    "build:brave": "BROWSER_TARGET=brave vite build",
    "build:chrome-v2": "BROWSER_TARGET=chrome-v2 vite build",
    "build:all": "npm run build:chrome && npm run build:firefox && npm run build:brave && npm run build:chrome-v2"
  },
  "keywords": [
    "chrome-extension",
    "cookie-manager",
    "typescript",
    "react"
  ],
  "author": "mra1k3r0",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.11",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.243",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.27",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
